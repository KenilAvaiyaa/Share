<% include ./partials/header.ejs %>
    <div class="w-full min-h-screen bg-zinc-800 px-10 py-8 ">
        <div class="cards flex flex-wrap gap-10">
            <% user.posts.forEach( (e)=> { %>
                <div class="card w-32">
                    <div class=" w-32 h-40 bg-sky-200 rounded-lg overflow-hidden">
                        <img class="w-full h-full object-cover" src="/images/uplode/<%= e.photo %>" alt="">
                    </div>
                    <h5 class="text-white text-sm"><%= e.title %></h5>
                </div>
            <% }); %>
        </div>
    </div>
<% include ./partials/footer.ejs %> 